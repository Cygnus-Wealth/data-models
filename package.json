{
  "name": "@cygnus-wealth/data-models",
  "version": "1.0.0",
  "description": "Shared TypeScript data models for CygnusWealth project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "test:unit": "vitest tests/unit --run",
    "test:integration": "vitest tests/integration --run",
    "test:property": "vitest tests/property-based --run",
    "lint": "eslint . --ext .ts",
    "api:extract": "api-extractor run --local --verbose",
    "api:check": "api-extractor run --verbose",
    "prepublishOnly": "npm run build && npm run api:check"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "types",
    "data-models",
    "financial-data",
    "cryptocurrency",
    "crypto",
    "blockchain",
    "defi",
    "portfolio",
    "multi-chain",
    "ethereum",
    "solana",
    "trading",
    "cex",
    "dex"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@microsoft/api-extractor": "^7.53.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "fast-check": "^4.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  }
}
